    "cols_1 = ['Orden', 'Provincia', 'Codigo', 'Departamento/Partido']\n",
    "\n",
    "# https://stackoverflow.com/a/44631639/570393\n",
    "df.select([count(when(isnan(c) | col(c).isNull(), c)).alias(c) for c in cols_1]).show()"
   ]
